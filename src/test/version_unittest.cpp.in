#include "park/version.h"

namespace park{

static_assert(VersionMajor == @Park_VERSION_MAJOR@,
    "Version mismatch between version.h and CMakeLists.txt");
static_assert(VersionMinor == @Park_VERSION_MINOR@,
    "Version mismatch between version.h and CMakeLists.txt");
static_assert(VersionPatch == @Park_VERSION_PATCH@,
    "Version mismatch between version.h and CMakeLists.txt");
    
} // namespace park
